{
  "name": "turistou-webapp-cra",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "react-app-rewired start",
    "build": "babel src/ -d lib/ & react-app-rewired build",
    "prepare": "npm run build",
    "test": "react-app-rewired test",
    "lint-staged": "lint-staged",
    "prettier": "npx prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,less}\"",
    "eslint": "npx eslint --ext .js,jsx src",
    "stylelint": "stylelint \"src/**/*.{scss,less}\"",
    "flow": "flow"
  },
  "dependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rowno/sparkline": "^4.0.0",
    "antd": "^3.26.20",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "auth0-js": "^9.14.0",
    "bootstrap-css-only": "^4.4.1",
    "classnames": "^2.2.6",
    "commitizen": "^3.1.1",
    "commitlint": "^11.0.0",
    "connected-react-router": "^6.8.0",
    "cz-conventional-changelog": "^3.3.0",
    "enquire-js": "^0.2.1",
    "flow-bin": "^0.137.0",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "history": "^4.10.1",
    "jwt-decode": "^3.0.0",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "nprogress": "^0.2.0",
    "peity-react": "^0.0.7",
    "qs": "^6.9.4",
    "rc-drawer": "^4.1.0",
    "react": "^17.0.2",
    "react-apollo": "^3.1.5",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^17.0.2",
    "react-editmask": "^4.0.3",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.8.8",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^3.4.3",
    "react-text-mask": "^5.4.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/preset-flow": "^7.13.13",
    "babel-core": "^6.26.3",
    "babel-plugin-import": "^1.13.1",
    "babel-plugin-react-intl": "^8.2.10",
    "babel-preset-flow": "^6.23.0",
    "customize-cra": "^0.9.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "husky": "^2.2.0",
    "less": "^3.12.2",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.5.1",
    "node-sass": "^4.14.1",
    "react-app-rewired": "^2.1.6",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "npx cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,scss,less}": [
      "npx prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "npx eslint --fix",
      "git add"
    ],
    "src/**/*.{scss,less}": [
      "npx stylelint",
      "git add"
    ]
  }
}

{
  "name": "turistou-webapp-cra",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "prepublish": "yarn run build",
    "test": "react-app-rewired test",
    "lint-staged": "lint-staged",
    "prettier": "npx prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,less}\"",
    "eslint": "npx eslint --ext .js,jsx src",
    "stylelint": "stylelint \"src/**/*.{scss,less}\"",
    "flow": "$(npm bin)/flow"
  },
  "dependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@rowno/sparkline": "^4.0.0",
    "antd": "^3.20.7",
    "apollo-boost": "^0.4.3",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.18",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "auth0-js": "^9.11.2",
    "bootstrap-css-only": "^4.3.1",
    "classnames": "^2.2.6",
    "commitizen": "^4.0.3",
    "commitlint": "^8.1.0",
    "connected-react-router": "^6.5.2",
    "cz-conventional-changelog": "^3.0.2",
    "enquire-js": "^0.2.1",
    "flow-bin": "^0.104.0",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "history": "^4.9.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "luxon": "^1.13.2",
    "nprogress": "^0.2.0",
    "peity-react": "^0.0.7",
    "qs": "^6.7.0",
    "rc-drawer": "^2.0.3",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.6",
    "react-editmask": "^4.0.3",
    "react-helmet": "^5.2.1",
    "react-intl": "^2.9.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0",
    "react-router-dom": "^5.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "3.0.1",
    "react-text-mask": "^5.4.3",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.5",
    "redux-thunk": "^2.3.0",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-import": "^1.12.0",
    "babel-preset-flow": "^6.23.0",
    "customize-cra": "^0.4.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "husky": "^3.0.2",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.1",
    "node-sass": "^4.12.0",
    "react-app-rewired": "^2.1.3",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,scss,less}": [
      "npx prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "npx eslint --fix",
      "git add"
    ],
    "src/**/*.{scss,less}": [
      "npx stylelint",
      "git add"
    ]
  }
}

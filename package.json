{
  "name": "turistou-webapp-cra",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "lint-staged": "lint-staged",
    "prettier": "npx prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,less}\"",
    "eslint": "npx eslint --ext .js,jsx src",
    "stylelint": "stylelint \"src/**/*.{scss,less}\""
  },
  "dependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@rowno/sparkline": "^3.0.1",
    "antd": "^3.12.1",
    "auth0-js": "^9.10.2",
    "bootstrap-css-only": "^4.2.1",
    "classnames": "^2.2.6",
    "commitizen": "^3.1.1",
    "commitlint": "^7.6.1",
    "connected-react-router": "^6.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "enquire-js": "^0.2.1",
    "eslint": "^5.16.0",
    "firebase": "^5.7.2",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "nprogress": "^0.2.0",
    "peity-react": "^0.0.7",
    "prop-types": "^15.6.2",
    "qs": "^6.6.0",
    "rc-drawer": "^1.7.7",
    "react": "^16.7.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.7.0",
    "react-editmask": "^4.0.1",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.7.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3",
    "react-router-dom": "^4.3",
    "react-router-redux": "^4.0.8",
    "react-scripts": "2.1.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "redux-thunk": "^2.3.0",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "babel-plugin-import": "^1.11.0",
    "customize-cra": "^0.2.9",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "husky": "^2.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.6",
    "node-sass": "^4.12.0",
    "react-app-rewired": "^2.1.3",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-standard": "^18.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "npx cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,scss,less}": [
      "npx prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "npx eslint --fix",
      "git add"
    ],
    "src/**/*.{scss,less}": [
      "npx stylelint",
      "git add"
    ]
  }
}
